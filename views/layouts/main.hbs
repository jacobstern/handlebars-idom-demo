<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width">
  <title>{{title }}</title>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bulma/0.7.4/css/bulma.min.css">
  <link rel="stylesheet" href="/css/style.css">
  {{#if ENV_DEVELOPMENT}}
    <script src="http://localhost:35729/livereload.js"></script>
  {{/if}}
</head>
<body>

  {{{body}}}

  <script>
    (function () {
      var partials = window.precompiledPartials = {};
      {{#each precompiled}}
        partials['{{@key}}'] = {{{this}}};
      {{/each}}
    }());
  </script>
  <script>
    {{#if pageDataJSON}}
      window.pageState = {{{pageDataJSON}}};
    {{/if}}
  </script>
  {{#each extraScripts}}
    <script src="{{{this}}}"></script>
  {{/each}}
</body>
</html>
